{
  "name": "first_rust",
  "version": "1.0.0",
  "description": "",
  "main": "frontend/index.js",
  "scripts": {
    "all": "npm run make; npm run deploy",
    "build": "npm run rust; webpack --config webpack.config.js",
    "deploy": "rm -rf deploy; mkdir deploy; npm run venv; bash setup.sh",
    "install": "npip install -r requirements.txt",
    "make": "npm i -f --no-package-lock --no-shrinkwrap; npm run build",
    "nopy": "nopy",
    "rust": "wasm-pack build -d frontend/pkg",
    "venv": "nopy -m venv --copies deploy/py_venv"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saminkhan/first_rust.git"
  },
  "author": "Samin Khan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saminkhan/first_rust/issues"
  },
  "homepage": "https://github.com/saminkhan/first_rust#readme",
  "devDependencies": {
    "html-webpack-plugin": "^3.2.0",
    "nopy": "^0.2.7",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "python": {
    "execPath": "python3"
  }
}
